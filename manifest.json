{
  "manifest_version": 3,
  "name": "QueryBee",
  "version": "1.0.0",
  "key": "",
  "permissions": [
    "cookies",
    "scripting",
    "activeTab",
    "storage",
    "sidePanel"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "dist/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["dist/content.js"]
    }
  ],
  "side_panel": {
    "default_path": "dist/sidebar.html"
  },
  "icons": {
    "16": "dist/icon.png",
    "48": "dist/icon.png",
    "128": "dist/icon.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "sidebar.html",
        "icon.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}